# eksisozluk/eksisozluk

[alias]
  l = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative

[branch "develop"]
  remote = origin
  merge = refs/heads/develop

[branch "master"]
  remote = origin
  merge = refs/heads/master

[color]
  ui = auto

[core]
  autocrlf = input

[fetch]
  prune = true

[gitflow "branch"]
  master = master
  develop = develop

[gitflow "prefix"]
  feature = feature/
  release = release/
  hotfix = hotfix/
  support = support/
  versiontag = v

[merge]
  ff = only

[push]
  default = matching

[rebase]
  autosquash = true

[remote "origin"]  
  url = git@github.com:eksisozluk/eksisozluk.git 
  fetch = +refs/heads/*:refs/remotes/origin/*
